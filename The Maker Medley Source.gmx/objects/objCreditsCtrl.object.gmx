var _state2AppearEnd = 1649;
var _state2StartTime = 1663;
var _state2SwitchInterval = 18;

// column "expanding"
if (tileState == 1){
    columnScale = clamp(max(floor(timer) - _state2AppearStart, 0)/(_state2AppearEnd - _state2AppearStart), 0, 1);
    reload = true;
}

