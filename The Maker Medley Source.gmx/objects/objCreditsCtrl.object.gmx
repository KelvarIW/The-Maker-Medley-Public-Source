        if (_state2Column &lt; 24){   //gauranteed
            for (var j=0; j&lt;15; j++){
                if (tilesV[j, 3] != _sharkIndex){
                    scrTitleDrawTiles(tilesV[j, 3], 4, 768, 32*(j+2));
