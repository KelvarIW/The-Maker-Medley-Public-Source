            }
        }
        if (_state2Column &lt; 0){
            for (var j=0; j&lt;15; j++){
                if (tilesV[j, 0] != _sharkIndex){
                    scrTitleDrawTiles(tilesV[j, 0], 4, 0, 32*(j+2));
                }else{
                    scrTitleDrawTiles(tilesV[j, 0], 3, 0, 32*(j+2));
