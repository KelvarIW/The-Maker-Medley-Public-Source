                    var _hit = true;
                    //prevent getting hit by an instakill twice
                    for (var i = (instasNeeded-1); i &gt;= 0; i--){
                        if (instaHit[i] == other.id){
                            _hit = false;
                            break;
                        }
                    }
                    if (_hit){
                        // store id to prevent double-hits
                        for (var i = (instasNeeded-1); i &gt; 0; i--){
                            instaHit[i] = instaHit[i-1];
                        }
                        instaHit[0] = other.id;
                        
                        // register hit
