                (instance_create((nextJumpX + 48), (nextJumpY - 32)  - 40, objFinalKurathSpikeDown)).ident = 1;
                instance_create((nextJumpX + 80), (nextJumpY - 32)  - 32, objFinalKurathSpikeUp);
